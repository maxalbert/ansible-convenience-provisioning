alias vib='vi ~/.bashrc'
alias viba='vi ~/.bash_aliases_custom'
alias sob='source ~/.bashrc'

alias l='ls -lh'
alias df='df -h'

alias gst='git status --untracked=no'
alias gstu='git status --untracked=normal'
alias gu='git status --untracked=all'
alias gd='git diff'
alias gdh='git diff HEAD'
alias gwd='git diff --word-diff'
alias gdc='git diff --cached'
alias gcom='git commit'
alias gam='git commit -a --amend'
alias gl='clear && git lg1 | head -n 50 && echo ""'
alias gc='git checkout'
alias gcp='git cherry-pick'
alias gui='git update-index'
alias gb='git branch'
alias gss='git stash save'
alias gsh='git show HEAD'
alias gpgh='git push'
